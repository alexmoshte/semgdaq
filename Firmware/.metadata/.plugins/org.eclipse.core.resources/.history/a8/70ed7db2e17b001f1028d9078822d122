#ifndef INC__ADCN_INX_MA_H_
#define INC__ADCN_INX_MA_H_

#include "_ADCn_INx_NF.h"

#define MA_FILTER_LENGTH 10

typedef struct
{
	float32_t _MA_ADC1_IN1_bfr[ MA_FILTER_LENGTH];

    uint32_t MA_ADC1_IN1_bfr_indx;

	float32_t MA_ADC1_IN1[ADC_DMA_HALFBUFFERSIZE];

} ADC1_IN1_MA;


void MA_ADC1_IN1_Init (ADC1_IN1_MA *ADC1_IN1_MA_ptr); //

float32_t MA_ADC1_IN1_Update (ADC1_IN1_MA *ADC1_IN1_MA_ptr, float* InpBfr, float* OutBfr, ADC_DMA_HALFBUFFERSIZE);

#endif /* INC__ADCN_INX_MA_H_ */
